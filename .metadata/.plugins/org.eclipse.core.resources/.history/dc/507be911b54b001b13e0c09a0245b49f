<suite name="testng">
	<test thread-count="5" name="stuff">
		<classes>
			<class name="testng.test" />
			<class name="testng.test2" />
		</classes>
	</test> <!-- stuff -->


	<test name="MoreStuff">
		<classes>
			<class name="testng.test2">
				<methods>
					<exclude name="Pass" />
				</methods>
			</class>
		</classes>
	</test>
	
	
		<test name="SomeMoreStuff">
		<classes>
			<class name="testng.test2">
				<methods>
					<include name="Pass" />
				</methods>
			</class>
			<class name="testng.test">
				<methods>
					<include name="Pass" />
				</methods>
			</class>
		</classes>
	</test>
</suite> <!-- testng -->
