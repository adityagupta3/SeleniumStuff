package testng;

import org.testng.annotations.Test;

public class test2 {

	@Test(enabled=false)
	public void check() {
		System.out.println("Just a check");
	}
	
	@Test(dependsonMethod="Pass"){
		System.out.println("test");
	}
	
	@Test
	public void testPrint() {
		System.out.println("TEST 2 TESTNG");
	}
	
	@Test(groups=("smoke"))
	public void Pass() {
		System.out.println("Pass 2");
		
	}
	
}
